    "babel-preset-react": "^6.1.4",
    "css-loader": "^0.21.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "node-sass": "^3.4.2",
    "react": ">=0.14.0 || >=15.0.0-0",
